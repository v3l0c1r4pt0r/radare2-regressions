NAME=ELF: entrypoint with baddr
FILE=../bins/elf/analysis/pie
ARGS=-B 0x300000
CMDS=<<EXPECT
iI~baddr
ie~program
EXPECT=<<RUN
baddr    0x300000
vaddr=0x00300450 paddr=0x00000450 laddr=0x00000000 haddr=0x00000018 hvaddr=0x00300018 type=program
RUN

NAME=ELF: rabin2 entrypoint with baddr
FILE=-
CMDS=<<EXPECT
!rabin2 -I -B 0x300000 ../bins/elf/analysis/pie | grep baddr
!rabin2 -e -B 0x300000 ../bins/elf/analysis/pie | sed -n '2 p'
EXPECT=<<RUN
baddr    0x300000
vaddr=0x00300450 paddr=0x00000450 laddr=0x00000000 haddr=0x00000018 hvaddr=0x00300018 type=program
RUN

