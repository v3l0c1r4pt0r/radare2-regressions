NAME=et
FILE=-
EXPECT=<<EOF
bool
int
str
addr
EOF
CMDS=<<EOF
et asm.slow
et asm.bits
et asm.arch
et search.from
EOF
RUN

NAME=e asm.os
FILE=../bins/elf/analysis/hello-linux-x86_64
EXPECT=<<EOF
ios
dos
darwin
linux
freebsd
openbsd
netbsd
windows
EOF
CMDS=<<EOF
e asm.os=?
EOF
RUN

NAME=e asm.cpu
FILE=../bins/elf/analysis/hello-linux-x86_64
EXPECT=<<EOF
mips32/64
micro
r6
v3
v2
EOF
CMDS=<<EOF
e asm.arch=mips
e asm.cpu=?
EOF
RUN

NAME=e asm.syntax
FILE=-
EXPECT=<<EOF
jmp 10h
EOF
CMDS=<<EOF
wx eb0e
e asm.arch=x86
e asm.syntax=masm
pi 1
EOF
RUN
