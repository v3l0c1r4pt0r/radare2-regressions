NAME=pib from begining of bb
FILE=../bins/elf/analysis/x64-loop
EXPECT=<<EOF
push rbp
mov rbp, rsp
sub rsp, 0x20
mov dword [rbp - 0x14], edi
mov qword [rbp - 0x20], rsi
mov dword [rbp - 4], 0
jmp 0x400523
EOF
CMDS=<<EOF
s main
af
pib
EOF
RUN

