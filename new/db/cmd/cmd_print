NAME=p2 hello-linux-x86_64
FILE=../bins/elf/analysis/hello-linux-x86_64
EXPECT=<<EOF
----..==----##..
--==####..####..
==..##..------==
--==####..####--
==..==##--##==##
..----####--====
==..==..########
##..##==--==##--
EOF
CMDS=<<EOF
p2 16
EOF
RUN

NAME=p= 0xff 0x00
FILE=-
BROKEN=1
EXPECT=<<EOF
         ___________________________________________________
00 00ff | __________________________________________________|
01 0000 ||
EOF
CMDS=<<EOF
b 2
wx 0xff
p= 2@0
EOF
RUN

NAME=p=b 0xff 0x00
FILE=-
EXPECT=<<EOF
0x00000000 00 00ff |'__________________________________________________|
0x00000001 01 0000 ||
EOF
CMDS=<<EOF
b 2
wx 0xff
e zoom.in=raw
p=b 2 2@0
EOF
RUN

NAME=p= - crash
FILE=-
EXPECT=<<EOF
0x00000000 00 0000 ||
EOF
CMDS=<<EOF
e zoom.in = block
p=p" 1 " 1 EN
EOF
RUN

